<?xml version="1.0" encoding="utf-8"?>
<quartz xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" overwrite-existing-jobs="true">
  <job>
    <job-detail>
      <name>SummaryExportJob</name>
      <group>TOPKJobs</group>
      <title>导出汇总文件</title>
      <description>根据汇总得到的数据生成并导出汇总文件，存储在共享目录上</description>
      <job-type>CFLD.TOPKTO.ServiceJobs.SummaryExportJob, CFLD.TOPKTO.ServiceJobs</job-type>
    </job-detail>
    <trigger>
      <simple>
        <name>SummaryExportJobTrigger</name>
        <group>TOPKJobsTriggers</group>
        <job-name>SummaryExportJob</job-name>
        <job-group>TOPKJobs</job-group>
        <start-time>2014-01-01T00:00:00</start-time>
        <end-time>9999-01-01T00:00:00</end-time>
        <repeat-count>-1</repeat-count>
        <repeat-interval>300000</repeat-interval>
      </simple>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>SMSJob</name>
      <group>CFLDJobs</group>
      <title>短信数据发送</title>
      <description>发送短信</description>
      <job-type>CFLD.TOPKTO.ServiceJobs.SMSService.SMSJob, CFLD.TOPKTO.ServiceJobs</job-type>
    </job-detail>
    <trigger>
      <simple>
        <name>SMSJobTrigger</name>
        <group>CFLDJobsTriggers</group>
        <job-name>SMSJob</job-name>
        <job-group>CFLDJobs</job-group>
        <start-time>2014-06-13T00:00:00</start-time>
        <end-time>9999-01-01T00:00:00</end-time>
        <repeat-count>-1</repeat-count>
        <repeat-interval>60000</repeat-interval>
      </simple>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>OAToDoJob</name>
      <group>CFLDJobs</group>
      <title>OA待办发送</title>
      <description>发送OA待办消息</description>
      <job-type>CFLD.TOPKTO.ServiceJobs.OAService.OAToDoJob, CFLD.TOPKTO.ServiceJobs</job-type>
    </job-detail>
    <trigger>
      <simple>
        <name>OAJobTrigger</name>
        <group>CFLDJobsTriggers</group>
        <job-name>OAToDoJob</job-name>
        <job-group>CFLDJobs</job-group>
        <start-time>2014-06-13T00:00:00</start-time>
        <end-time>9999-01-01T00:00:00</end-time>
        <repeat-count>-1</repeat-count>
        <repeat-interval>60000</repeat-interval>
      </simple>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>RTXJob</name>
      <group>CFLDJobs</group>
      <title>RTX消息发送</title>
      <description>发送RTX消息</description>
      <job-type>CFLD.TOPKTO.ServiceJobs.RTXService.RTXJob, CFLD.TOPKTO.ServiceJobs</job-type>
    </job-detail>
    <trigger>
      <simple>
        <name>RTXJobTrigger</name>
        <group>CFLDJobsTriggers</group>
        <job-name>RTXJob</job-name>
        <job-group>CFLDJobs</job-group>
        <start-time>2014-06-13T00:00:00</start-time>
        <end-time>9999-01-01T00:00:00</end-time>
        <repeat-count>-1</repeat-count>
        <repeat-interval>60000</repeat-interval>
      </simple>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>EsstateJob</name>
      <group>PaymentJobs</group>
      <title>地产数据获取</title>
      <description>获取地产回款及签约数据</description>
      <job-type>CFLD.TOPKTO.ServiceJobs.EsstateService.EsstateJob, CFLD.TOPKTO.ServiceJobs</job-type>
    </job-detail>
    <trigger>
      <cron>
        <name>EsstateJobTrigger</name>
        <group>PaymentJobsTriggers</group>
        <job-name>EsstateJob</job-name>
        <job-group>PaymentJobs</job-group>
        <cron-expression>0 0 12 * * ?</cron-expression>
      </cron>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>FinanceJob</name>
      <group>FinancePaymentJobs</group>
      <title>财务数据获取</title>
      <description>获取财务回款及支出数据</description>
      <job-type>CFLD.TOPKTO.ServiceJobs.FinanceService.FinanceJob, CFLD.TOPKTO.ServiceJobs</job-type>
    </job-detail>
    <trigger>
      <cron>
        <name>FinanceJobTrigger</name>
        <group>FinancePaymentJobsTriggers</group>
        <job-name>FinanceJob</job-name>
        <job-group>FinancePaymentJobs</job-group>
        <cron-expression>0 0 12 * * ?</cron-expression>
      </cron>
    </trigger>
  </job>
